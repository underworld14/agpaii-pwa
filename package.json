    "@ionic/react": "^6.1.12",
    "@ionic/react-router": "^6.1.12",
    "@prisma/client": "^4.0.0",
    "@trpc/client": "^9.25.3",
    "@trpc/next": "^9.25.3",
    "@trpc/react": "^9.25.3",
    "@trpc/server": "^9.25.3",
    "autoprefixer": "^10.2.6",
    "next": "^12.2.0",
    "postcss": "^8.3.5",
    "pullstate": "^1.24.0",
    "react": "^18.2.0",
